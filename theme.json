{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"blocks": {
			"core/group": {
				"color": {
					"background": true
				}
			}
		},
		"custom": {
			"color": {
				"primary": "#DD93B5",
				"secondary": "#F47733",
				"tertiary": "#0F5B66",
				"error": "#d4351c",
				"success": "#00703c",
				"focus": "#fd0",
				"white": "#fff",
				"black": "#000"
			},
			"layout": {
				"content": "1280px",
				"wide": "1536px",
				"padding": "var(--wp--preset--spacing--md)",
				"blockGap": "var(--wp--preset--spacing--sm)",
				"blockGapLarge": "var(--wp--preset--spacing--xl)"
			},
			"lineHeight": {
				"none": "1",
				"tight": "1.25",
				"snug": "1.375",
				"normal": "1.5",
				"relaxed": "1.625",
				"loose": "2"
			},
			"transitionDuration": {
				"quick": "100ms",
				"base": "250ms",
				"slow": "600ms"
			}
		},
		"color": {
			"background": false,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"link": false,
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "var(--wp--custom--color--primary)"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "var(--wp--custom--color--secondary)"
				},
				{
					"name": "Tertiary",
					"slug": "tertiary",
					"color": "var(--wp--custom--color--tertiary)"
				}
			],
			"text": false
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"spacingSizes": [
				{
					"name": "3XS",
					"size": "clamp(0.25rem, calc(0.15rem + 0.52vw), 0.56rem)",
					"slug": "3-xs"
				},
				{
					"name": "2XS",
					"size": "clamp(0.50rem, calc(0.38rem + 0.63vw), 0.88rem)",
					"slug": "2-xs"
				},
				{
					"name": "XS",
					"size": "clamp(0.75rem, calc(0.63rem + 0.63vw), 1.13rem)",
					"slug": "sm"
				},
				{
					"name": "Small",
					"size": "clamp(1.00rem, calc(0.77rem + 1.15vw), 1.69rem)",
					"slug": "sm"
				},
				{
					"name": "Medium",
					"size": "clamp(1.50rem, calc(1.25rem + 1.25vw), 2.25rem)",
					"slug": "md"
				},
				{
					"name": "Large",
					"size": "clamp(2.00rem, calc(1.54rem + 2.29vw), 3.38rem)",
					"slug": "lg"
				},
				{
					"name": "XL",
					"size": "clamp(3.38rem, calc(2.95rem + 2.14vw), 5.00rem)",
					"slug": "xl"
				},
				{
					"name": "2XL",
					"size": "clamp(3.00rem, calc(2.50rem + 2.50vw), 4.50rem)",
					"slug": "2-xl"
				},
				{
					"name": "3XL",
					"size": "clamp(4.00rem, calc(3.08rem + 4.58vw), 6.75rem)",
					"slug": "3-xl"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"name": "Sans",
					"slug": "sans",
					"fontFamily": "ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\""
				},
				{
					"name": "Serif",
					"slug": "serif",
					"fontFamily": "ui-serif, Georgia, Cambria, \"Times New Roman\", Times, serif"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"slug": "sm",
					"size": "14px",
					"fluid": {
						"min": "0.83rem",
						"max": "0.90rem"
					}
				},
				{
					"name": "Base",
					"slug": "base",
					"size": "18px",
					"fluid": {
						"min": "1.00rem",
						"max": "1.13rem"
					}
				},
				{
					"name": "Medium",
					"slug": "md",
					"size": "22px",
					"fluid": {
						"min": "1.20rem",
						"max": "1.41rem"
					}
				},
				{
					"name": "Large",
					"slug": "lg",
					"size": "28px",
					"fluid": {
						"min": "1.44rem",
						"max": "1.76rem"
					}
				},
				{
					"name": "XL",
					"slug": "xl",
					"size": "35px",
					"fluid": {
						"min": "1.73rem",
						"max": "2.20rem"
					}
				},
				{
					"name": "2XL",
					"slug": "2-xl",
					"size": "44px",
					"fluid": {
						"min": "2.07rem",
						"max": "2.75rem"
					}
				},
				{
					"name": "3XL",
					"slug": "3-xl",
					"size": "55px",
					"fluid": {
						"min": "2.49rem",
						"max": "3.43rem"
					}
				}
			],
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textColumns": true,
			"textDecoration": false,
			"textTransform": false
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--wp--custom--layout--block-gap)",
			"padding": {
				"right": "var(--wp--preset--spacing--md)",
				"left": "var(--wp--preset--spacing--md)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "1.9"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontWeight": "600"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3-xl)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2-xl)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)"
				}
			},
			"link": {
				"typography": {
					"textDecoration": "underline"
				}
			}
		}
	}
}
