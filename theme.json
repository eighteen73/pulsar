					"slug": "xl"
				},
				{
					"name": "64px",
					"size": "clamp(40px, 42.6667px + 1.6667vw, 64px)",
					"slug": "2-xl"
				},
				{
					"name": "80px",
					"size": "clamp(48px, 37.3333px + 3.3333vw, 80px)",
					"slug": "3-xl"
				},
				{
					"name": "96px",
					"size": "clamp(64px, 53.3333px + 3.3333vw, 96px)",
					"slug": "4-xl"
				},
				{
					"name": "112px",
					"size": "clamp(80px, 69.3333px + 3.3333vw, 112px)",
					"slug": "5-xl"
				},
				{
					"name": "128px",
					"size": "clamp(96px, 85.3333px + 3.3333vw, 128px)",
					"slug": "6-xl"
				},
				{
					"name": "154px",
