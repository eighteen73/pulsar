{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"spacing": {
			"blockGap": true,
      "defaultSpacingSizes": false,
      "customSpacingSize": false,
			"margin": true,
			"padding": true,
			"spacingSizes": [
				{
					"name": "2XS (4px)",
					"size": "4px",
					"slug": "2-xs"
				},
				{
					"name": "XS (8px)",
					"size": "8px",
					"slug": "xs"
				},
				{
					"name": "SM (12px)",
