					"fluid": {
						"min": "37px",
						"max": "50px"
					}
				},
        {
					"name": "4XL",
					"slug": "4-xl",
					"size": "63px",
					"fluid": {
						"min": "43px",
						"max": "63px"
					}
				}
			],
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textColumns": true,
			"textDecoration": false,
			"textTransform": false
		}
	}
}
