{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"custom": {
			"color": {
				"primitive": {
					"neutral": {
						"0": "#ffffff",
						"50": "#F5F6F7",
						"100": "#EDEFF1",
						"200": "#D8DCDF",
						"300": "#B5BEC4",
						"400": "#8D9AA3",
						"500": "#6F7E88",
						"600": "#5A666F",
						"700": "#49535B",
						"800": "#3F474D",
						"900": "#383D42",
						"950": "#25282C",
						"1000": "#000000"
					},
					"primary": {
						"50": "#F4F6FB",
						"100": "#E8ECF6",
						"200": "#CBD8EC",
						"300": "#9DB7DC",
						"400": "#6890C8",
						"500": "#4573B2",
						"600": "#335A96",
						"700": "#2B4879",
						"800": "#273E65",
						"900": "#253655",
						"950": "#090D15"
					},
					"secondary": {
						"50": "#F6F4FB",
						"100": "#ECE8F6",
						"200": "#D1CBEC",
						"300": "#A89DDC",
						"400": "#7868C8",
						"500": "#5745B2",
						"600": "#463396",
						"700": "#3B2B79",
						"800": "#342765",
						"900": "#302555",
						"950": "#0C0915"
					},
					"error": {
						"100": "#FFCDD2",
						"500": "#F44336",
						"900": "#B71C1C"
					},
					"warning": {
						"100": "#F9EACC",
						"500": "#EDB95E",
						"900": "#C75F18"
					},
					"success": {
						"100": "#E0F4D3",
						"500": "#67C23A",
						"900": "#29491E"
					}
				},
				"focus": "#fd0",
				"focus-accent": "var:custom|color|primitive|neutral|800",
				"error": "var:custom|color|primitive|error|500",
				"error-accent": "var:custom|color|primitive|error|100",
				"warning": "var:custom|color|primitive|warning|500",
				"warning-accent": "var:custom|color|primitive|warning|100",
				"success": "var:custom|color|primitive|success|500",
				"success-accent": "var:custom|color|primitive|success|100",
				"shadow": "var:custom|color|primitive|neutral|900",
				"border": "var:custom|color|primitive|neutral|500",
				"border-invert": "var:custom|color|primitive|neutral|200"
			},
			"global": {
				"is-rounded": 0,
				"is-dark-mode": 0
			},
			"layout": {
				"content": "768px",
				"wide": "1280px"
			},
			"spacing": {
				"blockGap": "var:preset|spacing|xl",
				"groupGap": "var:preset|spacing|5-xl",
				"rowGap": "var:preset|spacing|3-xl",
				"columnGap": "var:preset|spacing|3-xl",
				"edge": "clamp(var(--wp--preset--spacing--2-xl), 5.3333px + 5.8333vw, var(--wp--preset--spacing--5-xl))"
			},
			"border": {
				"radius": {
					"xs": "4px",
					"sm": "8px",
					"md": "clamp(10px, 1.5vw, 16px)",
					"lg": "clamp(16px, 2.5vw, 24px)",
					"xl": "clamp(24px, 3.5vw, 48px)",
					"2-xl": "clamp(48px, 4.5vw, 96px)",
					"full": "100%"
				}
			},
			"typography": {
				"weight": {
					"regular": 400,
					"bold": 600
				},
				"lineHeight": {
					"none": "1",
					"tight": "1.1",
					"snug": "1.375",
					"normal": "1.5",
					"relaxed": "1.6",
					"loose": "2"
				},
				"letterSpacing": {
					"tighter": "-0.05em",
					"tight": "-0.025em",
					"normal": "0em",
					"wide": "0.025em",
					"wider": "0.05em"
				}
			},
			"transition": {
				"duration": {
					"quick": "100ms",
					"normal": "250ms",
					"slow": "600ms"
				},
				"timing": {
					"ease-in": "ease-in",
					"ease-out": "ease-out"
				}
			}
		}
	}
}
