{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"custom": {
			"color": {
				"primitive": {
					"neutral": {
						"0": "#ffffff",
						"50": "#F5F6F7",
						"100": "#EDEFF1",
						"200": "#D8DCDF",
						"300": "#B5BEC4",
						"400": "#8D9AA3",
						"500": "#6F7E88",
						"600": "#5A666F",
						"700": "#49535B",
						"800": "#3F474D",
						"900": "#383D42",
						"950": "#25282C",
						"1000": "#000000"
					},
					"primary": {
						"50": "#F4F2FF",
						"100": "#EBE6FF",
						"200": "#DAD1FF",
						"300": "#BFACFF",
						"400": "#A17EFF",
						"500": "#8549FF",
						"600": "#7724FF",
						"700": "#6913EE",
						"800": "#580FC8",
						"900": "#4C0FA9",
						"950": "#2B066F"
					},
					"secondary": {
						"50": "#FAFEE7",
						"100": "#F3FCCB",
						"200": "#E7F99D",
						"300": "#D6F36E",
						"400": "#BDE635",
						"500": "#9FCC16",
						"600": "#7BA30D",
						"700": "#5D7C0F",
						"800": "#4B6212",
						"900": "#3F5314",
						"950": "#202E05"
					},
					"error": {
						"100": "#FFF1F2",
						"500": "#F44336",
						"900": "#B71C1C"
					},
					"warning": {
						"100": "#F9EACC",
						"500": "#EDB95E",
						"900": "#C75F18"
					},
					"success": {
						"100": "#E0F4D3",
						"500": "#67C23A",
						"900": "#29491E"
					}
				},
				"focus": "#fd0",
				"link": "var:custom|color|primitive|primary|500",
				"focus-accent": "var:custom|color|primitive|neutral|800",
				"error": "var:custom|color|primitive|error|900",
				"error-accent": "var:custom|color|primitive|error|100",
				"warning": "var:custom|color|primitive|warning|500",
				"warning-accent": "var:custom|color|primitive|warning|100",
				"success": "var:custom|color|primitive|success|900",
				"success-accent": "var:custom|color|primitive|success|100",
				"shadow": "var:custom|color|primitive|neutral|900",
				"border": "var:custom|color|primitive|neutral|100",
				"border-invert": "var:custom|color|primitive|neutral|200",
        "border-input": "var:custom|color|primitive|neutral|400",
        "border-input-invert": "var:custom|color|primitive|neutral|200"
			},
			"global": {
				"is-rounded": 0
			},
			"layout": {
				"content": "768px",
				"wide": "1280px"
			},
			"spacing": {
				"blockGap": "var:preset|spacing|xl",
				"groupGap": "var:preset|spacing|5-xl",
				"rowGap": "clamp(var(--wp--preset--spacing--2-xl), 5.3333px + 5.8333vw, var(--wp--preset--spacing--4-xl))",
				"columnGap": "var:preset|spacing|3-xl",
				"edge": "clamp(var(--wp--preset--spacing--2-xl), 5.3333px + 5.8333vw, var(--wp--preset--spacing--6-xl))"
			},
			"border": {
				"radius": {
					"xs": "4px",
					"sm": "8px",
					"md": "clamp(10px, 1.5vw, 16px)",
					"lg": "clamp(16px, 2.5vw, 24px)",
					"xl": "clamp(24px, 3.5vw, 48px)",
					"2-xl": "clamp(48px, 4.5vw, 96px)",
					"full": "100%"
				}
			},
      "shadow": {
        "xs": "0 1px 2px 0 hsl(from var(--wp--custom--color--shadow) h s l / 0.5)",
        "sm": "0 4px 6px -1px hsl(from var(--wp--custom--color--shadow) h s l / 0.1), 0 2px 4px -1px hsl(from var(--wp--custom--color--shadow) h s l / 0.06)",
        "md": "0 10px 15px -3px hsl(from var(--wp--custom--color--shadow) h s l / 0.1), 0 4px 6px -2px hsl(from var(--wp--custom--color--shadow) h s l / 0.05)",
        "lg": "0 20px 25px -5px hsl(from var(--wp--custom--color--shadow) h s l / 0.1), 0 10px 10px -5px hsl(from var(--wp--custom--color--shadow) h s l / 0.04)",
        "xl": "0 25px 50px -12px hsl(from var(--wp--custom--color--shadow) h s l / 0.25)"
      },
			"typography": {
				"fontWeight": {
					"body": 400,
					"heading": 700
				},
				"lineHeight": {
					"none": "1",
					"tight": "1.1",
					"snug": "1.375",
					"normal": "1.5",
					"relaxed": "1.6",
					"loose": "2"
				},
				"letterSpacing": {
					"tighter": "-0.05em",
					"tight": "-0.025em",
					"normal": "0em",
					"wide": "0.025em",
					"wider": "0.05em"
				}
			},
			"transition": {
				"duration": {
					"quick": "150ms",
					"normal": "300ms",
					"slow": "600ms"
				},
				"timing": {
					"ease-in": "ease-in",
					"ease-out": "ease-out"
				}
			}
		}
	}
}
